<AnchorPane prefHeight="200.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.SubscriberSearchFrameController">
   <children>
      <Label fx:id="labelMessage" layoutX="14.0" layoutY="20.0" prefHeight="30.0" prefWidth="372.0" text="Enter Subscriber ID:" />
      <TextField fx:id="idField" layoutX="14.0" layoutY="60.0" prefWidth="372.0" promptText="Subscriber ID" />
      <Button fx:id="btnSearch" layoutX="150.0" layoutY="120.0" mnemonicParsing="false" onAction="#handleSearchButton" text="Search" />
   </children>
</AnchorPane>
